{
  "version": 2,
  "name": "iremehub-server",
  "builds": [
    {
      "src": "server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "functions": {
    "server.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}